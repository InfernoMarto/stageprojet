{% if value %}
<div class="field field-url">
  {% if label %}<span class="field-label">{{ label|safe }}</span>{% endif %}

  {% set pending = value.indexOf('span class') > 0 %}
  {% if pending %}
    {{ value|safe }}
  {% else %}
   <a href="{{ value|add_protocol_if_need }}" target="_blank">{{ value|safe }}</a>
  {% endif %}
</div>
{% endif %}